<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/fondoito"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <LinearLayout
        android:layout_width="411dp"
        android:layout_height="86dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/nombre"
            android:layout_width="62dp"
            android:layout_height="wrap_content"
            android:text="N"
            android:textColor="#1D409A"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationX="30dp"
            android:translationY="20dp" />

        <TextView
            android:id="@+id/nom2"
            android:layout_width="34dp"
            android:layout_height="wrap_content"
            android:text="ET"
            android:textColor="#1D409A"
            android:textStyle="bold"
            android:translationX="-17dp"
            android:translationY="20dp" />

        <TextView
            android:id="@+id/nom3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="P"
            android:textColor="#1D409A"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationX="-32dp"
            android:translationY="20dp" />

        <TextView
            android:id="@+id/num4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="ULSE"
            android:textColor="#1D409A"
            android:textStyle="bold"
            android:translationX="-121dp"
            android:translationY="20dp" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="1dp"
            android:layout_height="52dp"
            android:layout_weight="1"
            android:backgroundTint="#00FFFFFF"
            android:translationX="-20dp"
            android:translationY="15dp"
            app:srcCompat="@mipmap/ic_foto1_foreground" />

    </LinearLayout>

    <androidx.cardview.widget.CardView
        android:layout_width="376dp"
        android:layout_height="530dp"
        android:translationY="80dp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="30dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView42"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="VerificaciÃ³n"
                android:textAlignment="center"
                android:textColor="#79A9BB"
                android:textSize="24sp"
                android:textStyle="bold"
                android:translationY="20dp" />

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="match_parent"
                android:layout_height="231dp"
                android:translationY="30dp"
                app:srcCompat="@mipmap/verificandoo" />

            <TextView
                android:id="@+id/textView43"
                android:layout_width="227dp"
                android:layout_height="wrap_content"
                android:text="@string/verificando"
                android:textAlignment="center"
                android:textSize="14sp"
                android:translationX="70dp"
                android:translationY="50dp" />

            <LinearLayout
                android:layout_width="300dp"
                android:layout_height="57dp"
                android:orientation="horizontal"
                android:translationX="35dp"
                android:translationY="60dp">

                <EditText
                    android:id="@+id/editTextNumber5"
                    android:layout_width="198dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/editTextNumber4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/editTextNumber2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/editTextNumber3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/editTextNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <Button
                android:id="@+id/button5"
                android:layout_width="195dp"
                android:layout_height="wrap_content"
                android:backgroundTint="#5A8CB4"
                android:text="Continuar"
                android:translationX="93dp"
                android:translationY="70dp" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>