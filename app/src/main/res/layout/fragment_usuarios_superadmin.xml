<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".fragmentos.superadmin.UsuariosSuperadminFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aclonica"
            android:text="FUNCIONALIDADES"
            android:textAlignment="center"
            android:textColor="#C3800D"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.042" />

        <TextView
            android:id="@+id/textView63"
            android:layout_width="326dp"
            android:layout_height="45dp"
            android:layout_marginTop="52dp"
            android:fontFamily="serif"
            android:text="Elige una de estas dos opciones "
            android:textColor="#090909"
            android:textSize="20sp"
            android:translationX="0dp"
            android:translationY="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintVertical_bias="0.053" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="125dp"
            android:layout_height="141dp"
            android:layout_marginStart="64dp"
            android:layout_marginBottom="316dp"
            android:backgroundTint="#80D3E1EA"
            android:translationX="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@mipmap/ic_usuarionuevo_foreground" />

        <Button
            android:id="@+id/NuevoUsuario"
            android:layout_width="148dp"
            android:layout_height="47dp"
            android:layout_marginEnd="84dp"
            android:layout_marginBottom="340dp"
            android:backgroundTint="#5A8CB4"
            android:onClick="nuevoUsuario"
            android:text="Nuevo Usuario"
            android:translationY="-20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="129dp"
            android:layout_height="128dp"
            android:layout_marginEnd="96dp"
            android:layout_marginBottom="132dp"
            android:backgroundTint="#E6F1E7"
            android:translationX="10dp"
            android:translationY="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@mipmap/ic_usuariooo_foreground" />

        <Button
            android:id="@+id/ListaUsuariosSuperadmin"
            android:layout_width="144dp"
            android:layout_height="55dp"
            android:layout_marginStart="72dp"
            android:layout_marginBottom="144dp"
            android:backgroundTint="#5AADB4"
            android:text="Lista de Usuarios"
            android:translationY="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>