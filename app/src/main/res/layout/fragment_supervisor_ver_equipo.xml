<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragmentos.supervisor.SupervisorVerEquipoFragment">

    <androidx.cardview.widget.CardView
        android:layout_width="420dp"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView52"
                android:layout_width="275dp"
                android:layout_height="63dp"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/aclonica"
                android:text="INFORMACIÓN"
                android:textAlignment="center"
                android:textColor="#3C8A82"
                android:textSize="34sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.503"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView36"
                android:layout_width="216dp"
                android:layout_height="222dp"
                android:layout_margin="25dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView52"
                app:srcCompat="@mipmap/ic_launcher_u_round" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/floatingActionButton6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="340dp"
                android:layout_marginTop="830dp"
                android:clickable="true"
                android:src="@drawable/ic_add"
                app:backgroundTint="#89CFC8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ScrollView
                android:layout_width="418dp"
                android:layout_height="511dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView36"
                tools:layout_editor_absoluteX="2dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginVertical="30dp"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="80dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView100"
                                android:layout_width="150dp"
                                android:layout_height="wrap_content"
                                android:text="Tipo de equipo"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                android:translationX="50dp" />

                            <TextView
                                android:id="@+id/tipoEquipo"
                                android:layout_width="215dp"
                                android:layout_height="wrap_content"
                                android:text="Tipo de equipo"
                                android:textSize="20sp"
                                android:translationX="60dp"
                                android:translationY="10dp" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="80dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView104"
                                android:layout_width="150dp"
                                android:layout_height="wrap_content"
                                android:text="Modelo"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                android:translationX="50dp" />

                            <TextView
                                android:id="@+id/modelo"
                                android:layout_width="215dp"
                                android:layout_height="wrap_content"
                                android:text="Modelo"
                                android:textSize="20sp"
                                android:translationX="60dp"
                                android:translationY="10dp" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="80dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView106"
                                android:layout_width="150dp"
                                android:layout_height="wrap_content"
                                android:text="Marca"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                android:translationX="50dp" />

                            <TextView
                                android:id="@+id/marca"
                                android:layout_width="292dp"
                                android:layout_height="wrap_content"
                                android:text="Marca"
                                android:textSize="20sp"
                                android:translationX="60dp"
                                android:translationY="10dp" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="80dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView108"
                                android:layout_width="150dp"
                                android:layout_height="wrap_content"
                                android:text="Número de serie"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                android:translationX="50dp" />

                            <TextView
                                android:id="@+id/numSeriee"
                                android:layout_width="215dp"
                                android:layout_height="wrap_content"
                                android:text="Número de serie"
                                android:textSize="20sp"
                                android:translationX="60dp"
                                android:translationY="10dp" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="80dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView109"
                                android:layout_width="150dp"
                                android:layout_height="wrap_content"
                                android:text="SKU"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                android:translationX="50dp" />

                            <TextView
                                android:id="@+id/sku"
                                android:layout_width="215dp"
                                android:layout_height="wrap_content"
                                android:text="SKU"
                                android:textSize="20sp"
                                android:translationX="60dp"
                                android:translationY="10dp" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="120dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView1081"
                                android:layout_width="150dp"
                                android:layout_height="wrap_content"
                                android:text="Descripción"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                android:translationX="50dp" />

                            <TextView
                                android:id="@+id/descripcion"
                                android:layout_width="215dp"
                                android:layout_height="wrap_content"
                                android:text="Descripción"
                                android:textSize="20sp"
                                android:translationX="60dp"
                                android:translationY="10dp" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                </LinearLayout>
            </ScrollView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</FrameLayout>