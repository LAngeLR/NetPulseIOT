<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/fondoito">

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="411dp"
        android:layout_height="86dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/nombre"
            android:layout_width="62dp"
            android:layout_height="wrap_content"
            android:text="N"
            android:textColor="#1D409A"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationX="30dp"
            android:translationY="20dp" />

        <TextView
            android:id="@+id/nom2"
            android:layout_width="34dp"
            android:layout_height="wrap_content"
            android:text="ET"
            android:textColor="#1D409A"
            android:textStyle="bold"
            android:translationX="-17dp"
            android:translationY="20dp" />

        <TextView
            android:id="@+id/nom3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="P"
            android:textColor="#1D409A"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationX="-32dp"
            android:translationY="20dp" />

        <TextView
            android:id="@+id/num4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="ULSE"
            android:textColor="#1D409A"
            android:textStyle="bold"
            android:translationX="-121dp"
            android:translationY="20dp" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="1dp"
            android:layout_height="52dp"
            android:layout_weight="1"
            android:backgroundTint="#00FFFFFF"
            android:translationX="-20dp"
            android:translationY="15dp"
            app:srcCompat="@mipmap/ic_foto1_foreground" />

    </LinearLayout>

    <androidx.cardview.widget.CardView
        android:layout_width="412dp"
        android:layout_height="630dp"
        android:translationY="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView28"
        app:layout_constraintVertical_bias="0.64">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="86dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView47"
                    android:layout_width="16dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Equipo 1"
                    android:textAlignment="textStart"
                    android:textColor="#3F51B5"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:translationX="30dp"
                    android:translationY="30dp" />

                <ImageView
                    android:id="@+id/imageView33"
                    android:layout_width="96dp"
                    android:layout_height="56dp"
                    android:layout_weight="1"
                    android:translationX="-70dp"
                    android:translationY="20dp"
                    app:srcCompat="@mipmap/telecoequi" />
            </LinearLayout>

            <TextView
                android:id="@+id/textView100"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Marca:"
                android:textSize="16sp"
                android:textStyle="bold"
                android:translationX="30dp" />

            <TextView
                android:id="@+id/textView101"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Telecom"
                android:textSize="16sp"
                android:translationX="50dp" />

            <TextView
                android:id="@+id/textView102"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Modelo:"
                android:textSize="16sp"
                android:textStyle="bold"
                android:translationX="30dp"
                android:translationY="15dp" />

            <TextView
                android:id="@+id/textView103"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Telecom"
                android:textSize="16sp"
                android:translationX="50dp"
                android:translationY="15dp" />

            <TextView
                android:id="@+id/textView105"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Descripción"
                android:textSize="16sp"
                android:textStyle="bold"
                android:translationX="30dp"
                android:translationY="20dp" />

            <TextView
                android:id="@+id/textView106"
                android:layout_width="336dp"
                android:layout_height="wrap_content"
                android:text="@string/descripción"
                android:translationX="50dp"
                android:translationY="20dp" />

            <TextView
                android:id="@+id/textView107"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Foto"
                android:textSize="16sp"
                android:textStyle="bold"
                android:translationX="30dp"
                android:translationY="40dp" />

            <ImageView
                android:id="@+id/imageView108"
                android:layout_width="match_parent"
                android:layout_height="164dp"
                android:translationY="60dp"
                app:srcCompat="@mipmap/equipo" />

            <ImageButton
                android:id="@+id/imageButton109"
                android:layout_width="wrap_content"
                android:layout_height="37dp"
                android:backgroundTint="#69BBA6"
                android:translationX="300dp"
                android:translationY="-460dp"
                app:srcCompat="@android:drawable/ic_menu_edit" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>